outfile="\\maple\data\Toronto\wrkgrp\wrkgrp33\RSD Files\DataIntegrity\Regulatory\COB\Bread\Data\Dashboard\Payplan_Dashboard_&mth_end2..xlsx"
dbms=xlsx replace;
sheet='Variables';
run;

